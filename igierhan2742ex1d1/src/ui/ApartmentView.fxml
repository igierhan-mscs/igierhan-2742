<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="380.0" prefWidth="375.0" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ui.ApartmentController">
   <children>
      <GridPane layoutX="60.0" layoutY="64.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="200.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="Apartment:" />
            <Label text="Apartment Num:" GridPane.rowIndex="1" />
            <Label text="Square feet:" GridPane.rowIndex="2" />
            <Label text="Bathrooms:" GridPane.rowIndex="3" />
            <Label text="Price:" GridPane.rowIndex="4" />
            <Label text="Updated:" GridPane.rowIndex="5" />
            <Label text="Administrator:" GridPane.rowIndex="6" />
            <Label text="Tenant:" GridPane.rowIndex="7" />
            <ComboBox fx:id="aptComboBox" onAction="#aptComboBox_ItemSelected" prefWidth="200.0" GridPane.columnIndex="1" />
            <ComboBox fx:id="adminComboBox" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="6" />
            <ComboBox fx:id="tenantComboBox" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="7" />
            <Button fx:id="saveAptButton" mnemonicParsing="false" onAction="#saveAptButton_Clicked" text="Save Apartment" GridPane.columnIndex="1" GridPane.rowIndex="8" />
            <Button fx:id="viewInvoiceDlgButton" mnemonicParsing="false" onAction="#viewInvoiceDlgButton_Clicked" text="View Invoices" GridPane.columnIndex="1" GridPane.rowIndex="9" />
            <TextField fx:id="apartmentNumTextField" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <TextField fx:id="squareFeetTextField" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <TextField fx:id="bathroomsTextField" GridPane.columnIndex="1" GridPane.rowIndex="3" />
            <TextField fx:id="priceTextField" GridPane.columnIndex="1" GridPane.rowIndex="4" />
            <TextField fx:id="updatedTextField" disable="true" focusTraversable="false" GridPane.columnIndex="1" GridPane.rowIndex="5" />
         </children>
      </GridPane>
   </children>
</AnchorPane>
